{
    "root": true,
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
    },
    "ignorePatterns": ["src/**/*.test.ts", "src/frontend/generated/*"],
    "env": {
      "browser": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 14,
      "sourceType": "commonjs"
    }
  }
  